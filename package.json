    "gulp-if": "0.0.5",
    "gulp-imagemin": "^0.6.2",
    "gulp-livereload": "~1.2.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-notify": "^1.4.2",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "~1.3.1",
    "gulp-sourcemaps": "^1.2.8",
